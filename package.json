{
  "name": "minelev-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "demo": "REACT_APP_IS_MOCK=true react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i && npm i",
    "standard-fix": "standard --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@sentry/react": "6.15.0",
    "@sentry/tracing": "6.15.0",
    "@vtfk/components": "0.2.1",
    "@vtfk/react-msal": "2.0.2",
    "express": "4.17.2",
    "get-skole-aar": "2.1.0",
    "moment": "2.29.1",
    "nanoid": "3.1.30",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-collapse": "5.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "7.12.2",
    "react-moment": "1.1.1",
    "react-notifications-component": "3.1.0",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-scrolllock": "5.0.1",
    "sass": "1.49.0",
    "vtfk-schools-info": "3.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "axios": "0.24.0",
    "msw": "0.36.3",
    "react-test-renderer": "17.0.2",
    "standard": "16.0.4"
  }
}
