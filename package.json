{
  "name": "vtfk-minelev-react",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "demo": "REACT_APP_IS_MOCK=true react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "standard-fix": "standard --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@sentry/react": "6.0.1",
    "@sentry/tracing": "6.0.1",
    "@vtfk/react-msal": "1.0.6",
    "express": "4.17.1",
    "get-skole-aar": "2.1.0",
    "moment": "2.29.1",
    "nanoid": "3.1.20",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-collapse": "5.1.0",
    "react-datepicker": "3.4.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "3.1.0",
    "react-hook-form": "6.14.2",
    "react-moment": "1.1.1",
    "react-notifications-component": "3.0.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-scrolllock": "5.0.1",
    "sass": "1.32.5",
    "vtfk-schools-info": "1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "5.0.0",
    "@testing-library/user-event": "12.6.2",
    "axios": "0.21.1",
    "msw": "0.25.0",
    "react-test-renderer": "17.0.1",
    "standard": "16.0.3"
  }
}
